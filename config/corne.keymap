#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            // -----------------------------------------------------------------------------------------
            // |  TAB |  Q  |  W  |  E  |  R  |  T  |   |  Y   |  U   |  I  |  O  |  P  | DEL |
            // | CTRL |  A  |  S  |  D  |  F  |  G  |   |  H   |  J   |  K  |  L  |  ;  |  '   |
            // | SHFT |  Z  |  X  |  C  |  V  |  B  |   |  N   |  M   |  ,  |  .  |  /  | PGDN  |
            //                    | CMD | RET | L1  |   | BSPC | SPC  | L2 |

            bindings = <
&kp TAB         &kp Q  &kp W  &kp E     &kp R       &kp T    &kp Y          &kp U      &kp I      &kp O    &kp P     &kp DEL
&kp LEFT_SHIFT  &kp A  &kp S  &kp D     &kp F       &kp G    &kp H          &kp J      &kp K      &kp L    &kp SEMI  &kp SQT
&kp LCTRL       &kp Z  &kp X  &kp C     &kp V       &kp B    &kp N          &kp M      &kp COMMA  &kp DOT  &kp FSLH  &kp PAGE_DOWN
                              &kp LGUI  &kp RETURN  &mo 1    &kp BACKSPACE  &kp SPACE  &mo 2
            >;
        };

        lower_layer {
            // -----------------------------------------------------------------------------------------
            // |      |  1  |  2  |  3  |  4  |  5  |   |     |  7  |  8  |  9  |     |      |
            // |      |     |left | rght| up  |     |   |     |  4  |  5  |  6  |     |      |
            // |      |     |     |     | dwn |     |   |     |  1  |  2  |  3  |     |      |
            //                    | CMD |     | SPC |   | ENT |     | 0  |

            bindings = <
&none     &kp N1  &kp N2          &kp N3     &kp N4        &kp N5       &none    &kp N7        &kp N8        &kp N9        &none   &none
&none     &none   &kp LEFT_ARROW  &kp RIGHT  &kp UP_ARROW  &none        &none    &kp NUMBER_4  &kp N5        &kp NUMBER_6  &trans  &trans
&kp LALT  &trans  &trans          &trans     &kp DOWN      &trans       &trans   &kp NUMBER_1  &kp NUMBER_2  &kp NUMBER_3  &trans  &trans
                                  &kp LGUI   &trans        &kp SPACE    &kp RET  &trans        &kp NUMBER_0
            >;
        };

        raise_layer {
            // -----------------------------------------------------------------------------------------
            // |  TAB |  !  |  @  |  #  |  $  |  %  |   |  ^  |  &  |  *  |  (  |  )  | BSPC |
            // | SHFT |     |     |     |     |     |   |  -  |  =  |  [  |  ]  |  \  |  `   |
            // | CTRL |     |     |     |     |     |   |  _  |  +  |  {  |  }  | "|" |  ~   |
            //                    | GUI |     | SPC |   | ENT |     | ALT |

            bindings = <
&kp ESC       &kp EXCL  &kp AT  &kp HASH  &kp DLLR         &kp PRCNT           &kp CARET  &kp AMPS   &kp ASTRK  &kp LPAR  &kp RPAR  &kp BSPC
&kp CAPS      &trans    &trans  &trans    &kp C_VOLUME_UP  &kp C_PLAY_PAUSE    &kp MINUS  &kp EQUAL  &kp LBKT   &kp RBKT  &kp BSLH  &kp GRAVE
&kp LEFT_ALT  &trans    &trans  &trans    &kp C_VOL_DN     &trans              &kp UNDER  &kp PLUS   &kp LBRC   &kp RBRC  &kp PIPE  &kp TILDE
                                &kp LGUI  &trans           &kp SPACE           &kp RET    &trans     &kp RALT
            >;
        };
    };
};
